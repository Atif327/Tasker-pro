{
  "version": 2,
  "name": "flutter-tasker-pro-docs",
  "builds": [
    {
      "src": "docs/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/privacy-policy.html",
      "dest": "/docs/privacy-policy.html"
    },
    {
      "src": "/terms-and-conditions.html",
      "dest": "/docs/terms-and-conditions.html"
    },
    {
      "src": "/",
      "dest": "/docs/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
